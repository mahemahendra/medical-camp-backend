{
  "name": "medical-camp-backend",
  "version": "1.0.0", 
  "description": "Medical camp management backend API - Multi-tenant Node.js server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node --transpile-only src/index.ts",
    "dev:watch": "ts-node-dev --respawn --transpile-only src/index.ts",
    "seed:admin": "ts-node --transpile-only src/seed-admin.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "db:migrate": "node -r dotenv/config node_modules/.bin/typeorm migration:run",
    "db:generate": "node -r dotenv/config node_modules/.bin/typeorm migration:generate"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5", 
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.7",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.17",
    "validator": "^13.15.26"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5", 
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/qrcode": "^1.5.5",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "keywords": ["medical", "camp", "multi-tenant", "api", "nodejs", "postgresql"],
  "author": "Medical Camp Manager",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
